{
  "containerDefinitions": [
    {
      "portMappings": [
        {
          "hostPort": 4000,
          "protocol": "tcp",
          "containerPort": 4000
        }
      ],
      "environment": [
        {
          "name": "IMAGE",
          "value": "<IMAGE1_NAME>"
        },
        {
          "name": "COMMIT",
          "value": "<COMMIT>"
        },
        {
          "name": "BRANCH",
          "value": "<BRANCH>"
        },
        {
          "name": "ENVIRONMENT",
          "value": "server"
        },
        {
          "name": "PORT",
          "value": "<SERVICE_PORT>"
        },
        {
          "name": "NODE_ENV",
          "value": "development"
        }
      ],
      "image": "<IMAGE1_NAME>:dev",
      "name": "<CONTAINER_NAME>"
    }
  ],
  "executionRoleArn": "arn:aws:iam::<AWS_ACCOUNT_ID>:role/ECSTask",
  "taskRoleArn": "arn:aws:iam::<AWS_ACCOUNT_ID>:role/ECSTask",
  "family": "<APP_NAME>",
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512"
}
