{
  "containerDefinitions": [
    {
      "portMappings": [
        {
          "hostPort": 3000,
          "protocol": "tcp",
          "containerPort": 3000
        }
      ],
      "environment": [
        {
          "name": "IMAGE",
          "value": "<IMAGE1_NAME>"
        },
        {
          "name": "COMMIT",
          "value": "<COMMIT>"
        },
        {
          "name": "BRANCH",
          "value": "<BRANCH>"
        },
        {
          "name": "PORT",
          "value": "<SERVICE_PORT>"
        }
      ],
      "image": "<IMAGE1_NAME>",
      "name": "<CONTAINER_NAME>"
    }
  ],
  "executionRoleArn": "arn:aws:iam::<AWS_ACCOUNT_ID>:role/<APP_NAME>-ecs-exec-role",
  "taskRoleArn": "arn:aws:iam::<AWS_ACCOUNT_ID>:role/<APP_NAME>-ecs-task-role",
  "family": "<APP_NAME>-task",
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512"
}
